{
  "id": "se.warting.nfctools",
  "permissions": [
    "homey:wireless:nfc"
  ],
  "sdk": 2,
  "name": {
    "en": "NFC tools"
  },
  "description": {
    "en": "NFC tools"
  },
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "category": "tools",
  "version": "0.0.2",
  "compatibility": ">=1.5.0",
  "author": {
    "name": "Stefan Wärting",
    "email": "stefan@warting.se"
  },
  "contributors": [{
    "name": "Jimmy Wärting",
    "email": "jimmy@warting.se"
  }],
  "flow": {
    "triggers": [{
      "id": "nfc_touched",
      "title": {
        "en": "NFC touched"
      },
      "tokens": [{
        "name": "url",
        "type": "string",
        "title": {
          "en": "url"
        },
        "example": "https://www.youtube.com/watch?v=vFFDArGuLBg"
      }]
    }]
  }
}
